#include "pi.def".

% Tests 0 and 1 should succeed, test 2 should fail.
Define test by
  test 0 := bisim dummy_agent (in a X\ in a Y\ z) (in a X\ nu W\ in a Y\ z);
  test 1 := bisim dummy_agent (in a X\ nu Y\ match X Y (out c c z)) (in a X\ z);
  test 2 := bisim dummy_agent (nu X\ out a X (in c Y\ match X Y (out c c z)))
                (nu X\ out a X (in c Y\ z)).

#assert (test 0).
#assert (test 1).
#assert_not (test 2).
